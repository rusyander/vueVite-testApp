{
  "name": "vuevite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run serve\" ",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve": "json-server --watch db.json --port 5000"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "json-server": "^0.17.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7",
    "concurrently": "^6.5.1"
  }
}
